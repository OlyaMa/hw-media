{
  "plugins": ["prettier", "@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:@typescript-eslint/recommended"],
  "env": {
    "es2023": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [],
  "parser": "@typescript-eslint/parser"
}
